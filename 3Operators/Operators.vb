Module Operators

Sub Main()

Dim int1 As Integer = 4
Dim int2 As Integer = 5


 Console.WriteLine("Multiplication "+(int1*int2).ToString)       ' Multiplication *  
 Console.WriteLine("Division "+(int1/int2).ToString)             ' Division       /
 Console.WriteLine("Difference "+(int1-int2).ToString)           ' Sum Difference +,-
 Console.WriteLine("Modulus "+(int1 Mod int2).ToString)          ' Modulus        Mod eg (4 Mod 3)
 Console.WriteLine("Power "+(int1^int2).ToString)                ' Power          ^ eg (5^8)

        
        
Console.Readline()      'Wait for user 

End Sub
    
End Module